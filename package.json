{
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npx tsc && npx sass src/assets/styles:dist/assets/styles",
    "start": "node dist/server/index.js",
    "dev": "npm run build && npm run start",
    "test": "vitest run --coverage=false",
    "test:watch": "vitest --coverage=false",
    "coverage": "npx vitest --coverage run",
    "coverage:watch": "npx vitest --coverage"
  },
  "type": "module",
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@types/express": "^5.0.3",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sass": "^1.93.0"
  }
}
